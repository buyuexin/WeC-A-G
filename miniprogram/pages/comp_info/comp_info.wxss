.title{
  padding: 5% 10%;
  padding-bottom: 3%;
  text-align: center;
  font-weight: 500;
  font-size: 24px;
}

.item{
  background-color: var(--white);
	padding: 1rpx 10rpx;
  display: flex;
  flex-direction: row;
	align-items: center;
	min-height: 100rpx;
}

.item-left{
  width: 20%;
  /* text-align: center; */
}

.item-right{
  width: 80%;
  padding-left: 8rpx;
}

.item-40{
  width: 30%;
  /* text-align: center; */
}

.item-60{
  width: 70%;
  padding-left: 8rpx;
}

.item-50{
  flex: 1;
  background-color: var(--white);
	padding: 1rpx 10rpx;
  display: flex;
  flex-direction: row;
	align-items: center;
	min-height: 100rpx;
}

.border-right{
  border-right: 1px solid #f0f0f0;
  height: 60rpx;
}

.border-bottom{
  border-bottom: 2px solid var(--blue);
}

.height{
  height: 44px;
  line-height: 44px;
}

.collect{
  display: flex;
  justify-content: flex-end;
  margin-bottom: 5rpx;
  width: 100%;
}
.collect>view{
  padding-right: 10rpx;
  width: 125rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 15rpx;
  border: 1px solid var(--red);
  color: var(--red);
}

.bottomline{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 200rpx;
  color: var(--gray);
}

.bottomline>image{
  width: 300rpx;
}   

.con{
  word-wrap: break-word;
  word-break: normal;
  white-space:pre-wrap;
  margin-top: 16rpx;
} 
.add{
	position: fixed;
	width: 140rpx;
  bottom: 50rpx;
  right: 50rpx;
  border-radius: 50%;
	z-index: 1024;
	box-shadow: 3rpx 3rpx 3rpx 3rpx rgb(206, 206, 206);
}
.add>image{
  width: 140rpx;
  height: 140rpx;
  background-color: #fff;
  border-radius: 50%;
  display: block;
  box-shadow: 3rpx 3rpx 3rpx 3rpx rgb(206, 206, 206);
}